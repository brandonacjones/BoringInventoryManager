<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LogController">
   <children>
      <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox>
               <children>
                  <StackPane>
                     <children>
                        <Button mnemonicParsing="false" text="Filter">
                           <padding>
                              <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                           </padding></Button>
                     </children>
                  </StackPane>
                  <StackPane>
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Search">
                           <StackPane.margin>
                              <Insets left="5.0" right="5.0" />
                           </StackPane.margin></Text>
                     </children>
                  </StackPane>
                  <StackPane>
                     <children>
                        <TextField fx:id="fx_logfield" onAction="#search">
                           <padding>
                              <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                           </padding></TextField>
                     </children>
                  </StackPane>
               </children>
               <padding>
                  <Insets bottom="3.0" left="5.0" top="3.0" />
               </padding>
            </HBox>
            <ScrollPane fitToHeight="true" fitToWidth="true" VBox.vgrow="ALWAYS">
               <content>
                  <TableView fx:id="fx_logtable">
                    <columns>
                      <TableColumn fx:id="fx_timecol" prefWidth="200.0" text="Time" />
                      <TableColumn fx:id="fx_typecol" prefWidth="100.0" text="Type" />
                        <TableColumn fx:id="fx_usercol" prefWidth="300.0" text="User" />
                        <TableColumn fx:id="fx_descriptioncol" minWidth="-1.0" prefWidth="350.0" text="Description" />
                    </columns>
                  </TableView>
               </content>
            </ScrollPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
