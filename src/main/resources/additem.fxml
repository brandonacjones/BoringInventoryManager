<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddController">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <StackPane alignment="BOTTOM_LEFT">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="All fields are required.*">
                     <StackPane.margin>
                        <Insets left="20.0" />
                     </StackPane.margin>
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Text>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets top="20.0" />
               </padding>
            </StackPane>
            <AnchorPane VBox.vgrow="ALWAYS">
               <children>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <children>
                        <VBox alignment="CENTER_LEFT" HBox.hgrow="ALWAYS">
                           <children>
                              <StackPane alignment="CENTER_LEFT" prefHeight="150.0" prefWidth="200.0">
                                 <children>
                                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Part Number" />
                                 </children>
                              </StackPane>
                              <StackPane alignment="CENTER_LEFT" prefHeight="150.0" prefWidth="200.0">
                                 <children>
                                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Test Number" />
                                 </children>
                              </StackPane>
                              <StackPane alignment="CENTER_LEFT" prefHeight="150.0" prefWidth="200.0">
                                 <children>
                                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Serial Number" />
                                 </children>
                              </StackPane>
                              <StackPane alignment="CENTER_LEFT" prefHeight="150.0" prefWidth="200.0">
                                 <children>
                                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Quantity" />
                                 </children>
                              </StackPane>
                              <StackPane alignment="CENTER_LEFT" prefHeight="150.0" prefWidth="200.0">
                                 <children>
                                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Allocation" />
                                 </children>
                              </StackPane>
                              <StackPane alignment="CENTER_LEFT" prefHeight="150.0" prefWidth="200.0">
                                 <children>
                                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Purchase Order" />
                                 </children>
                              </StackPane>
                              <StackPane alignment="CENTER_LEFT" prefHeight="150.0" prefWidth="200.0">
                                 <children>
                                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Description" />
                                 </children>
                              </StackPane>
                              <StackPane alignment="CENTER_LEFT" prefHeight="150.0" prefWidth="200.0">
                                 <children>
                                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Location" />
                                 </children>
                              </StackPane>
                           </children>
                        </VBox>
                        <VBox prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
                           <children>
                              <StackPane prefHeight="150.0" prefWidth="200.0">
                                 <children>
                                    <TextField fx:id="fx_partnumberfield" promptText="XXX-XXXXXXXX">
                                       <StackPane.margin>
                                          <Insets bottom="5.0" top="5.0" />
                                       </StackPane.margin>
                                    </TextField>
                                 </children>
                              </StackPane>
                              <StackPane prefHeight="150.0" prefWidth="200.0">
                                 <children>
                                    <TextField fx:id="fx_testnumberfield" promptText="TN">
                                       <StackPane.margin>
                                          <Insets bottom="5.0" top="5.0" />
                                       </StackPane.margin>
                                    </TextField>
                                 </children>
                              </StackPane>
                              <StackPane prefHeight="150.0" prefWidth="200.0">
                                 <children>
                                    <TextField fx:id="fx_serialnumberfield" promptText="SN">
                                       <StackPane.margin>
                                          <Insets bottom="5.0" top="5.0" />
                                       </StackPane.margin>
                                    </TextField>
                                 </children>
                              </StackPane>
                              <StackPane prefHeight="150.0" prefWidth="200.0">
                                 <children>
                                    <TextField fx:id="fx_quantityfield" promptText="QTY">
                                       <StackPane.margin>
                                          <Insets bottom="5.0" top="5.0" />
                                       </StackPane.margin>
                                    </TextField>
                                 </children>
                              </StackPane>
                              <StackPane prefHeight="150.0" prefWidth="200.0">
                                 <children>
                                    <TextField fx:id="fx_allocationfield" promptText="ALLOC">
                                       <StackPane.margin>
                                          <Insets bottom="5.0" top="5.0" />
                                       </StackPane.margin>
                                    </TextField>
                                 </children>
                              </StackPane>
                              <StackPane prefHeight="150.0" prefWidth="200.0">
                                 <children>
                                    <TextField fx:id="fx_purchaseorderfield" promptText="PO">
                                       <StackPane.margin>
                                          <Insets bottom="5.0" top="5.0" />
                                       </StackPane.margin>
                                    </TextField>
                                 </children>
                              </StackPane>
                              <StackPane prefHeight="150.0" prefWidth="200.0">
                                 <children>
                                    <TextField fx:id="fx_descriptionfield" promptText="DESC">
                                       <StackPane.margin>
                                          <Insets bottom="5.0" top="5.0" />
                                       </StackPane.margin>
                                    </TextField>
                                 </children>
                              </StackPane>
                              <StackPane prefHeight="150.0" prefWidth="200.0">
                                 <children>
                                    <TextField fx:id="fx_locationfield" promptText="XX-XXXX-XX-XX">
                                       <StackPane.margin>
                                          <Insets bottom="5.0" top="5.0" />
                                       </StackPane.margin>
                                    </TextField>
                                 </children>
                              </StackPane>
                           </children>
                        </VBox>
                     </children>
                  </HBox>
               </children>
               <padding>
                  <Insets left="20.0" right="20.0" />
               </padding>
            </AnchorPane>
            <ButtonBar prefHeight="40.0" prefWidth="200.0">
              <buttons>
                  <Button fx:id="fx_cancelbutton" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
                <Button fx:id="fx_submitbutton" mnemonicParsing="false" onAction="#submit" text="Submit" />
              </buttons>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </ButtonBar>
         </children>
      </VBox>
   </children>
</AnchorPane>
